{
  "name": "unocss-ui",
  "type": "module",
  "private": false,
  "version": "0.0.1",
  "description": "A simple unocss vue3 ui lib.",
  "keywords": [
    "ui",
    "vue3",
    "unocss",
    "component",
    "simple"
  ],
  "author": "LU",
  "license": "MIT",
  "homepage": "https://github.com/cherryful/unocss-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/cherryful/unocss-ui",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/cherryful/unocss-ui/issues"
  },
  "main": "./dist/unocss-ui.umd.js",
  "module": "./dist/unocss-ui.esm.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/unocss-ui.es.js",
      "require": "./dist/unocss-ui.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.51",
    "@iconify-json/mi": "^1.1.4",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@vitejs/plugin-vue": "^4.2.2",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "sass": "^1.62.1",
    "unocss": "^0.51.12",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.4"
  }
}