{
  "name": "@unocss-ui/components",
  "type": "module",
  "version": "0.0.2-alpha.5",
  "private": false,
  "description": "A simple vue3 ui lib built with unocss.",
  "author": "cherryful <https://github.com/cherryful>",
  "license": "MIT",
  "homepage": "https://github.com/cherryful/unocss-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/cherryful/unocss-ui",
    "directory": "components"
  },
  "bugs": {
    "url": "https://github.com/cherryful/unocss-ui/issues"
  },
  "keywords": [
    "ui",
    "vue3",
    "unocss",
    "unocss ui",
    "component",
    "component library"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/components.umd.js",
      "import": "./dist/components.es.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "lodash-es": "^4.17.21",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.51",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@types/lodash-es": "^4.17.7",
    "@unocss-ui/preset": "workspace:*",
    "@vitejs/plugin-vue": "^4.2.2",
    "sass": "^1.62.1",
    "unocss": "^0.51.12",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
